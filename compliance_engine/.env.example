# Compliance Engine Configuration
# ================================
# Copy this file to .env and update values as needed

# Application Settings
APP_NAME=Compliance Engine
APP_VERSION=5.0.0
ENVIRONMENT=development
LOG_LEVEL=INFO

# Database Settings (FalkorDB)
FALKORDB_HOST=localhost
FALKORDB_PORT=6379
FALKORDB_PASSWORD=
RULES_GRAPH_NAME=RulesGraph
DATA_GRAPH_NAME=DataTransferGraph
TEMP_GRAPH_PREFIX=TempGraph_

# API Settings
API_HOST=0.0.0.0
API_PORT=5001
API_DEBUG=false
API_RELOAD=false
API_WORKERS=4
CORS_ORIGINS=["*"]
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60
DEFAULT_QUERY_TIMEOUT=60000
MAX_QUERY_TIMEOUT=120000

# Cache Settings
ENABLE_CACHE=true
CACHE_TTL=300
MAX_CACHE_SIZE=1000

# AI Service Settings
# Token Generation API
AI_TOKEN_API_URL=https://cmb-ib2b-dsp-sjdf/ib2b_tokenTranslator?_action=translate
AI_TOKEN_USERNAME=your_username_here
AI_TOKEN_PASSWORD=your_password_here

# LLM API
AI_LLM_API_URL=https://gapi-api.com/v1/api/v1/chat/completions
AI_LLM_MODEL=gpt-4.1
AI_USE_CASE_ID=compliance-engine
AI_TOKEN_EXPIRY_BUFFER=300

# AI Feature Flags
ENABLE_AI_RULE_GENERATION=true
ENABLE_AI_RULE_TESTING=true
